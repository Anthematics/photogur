<!DOCTYPE html>
<html>
<head>
	<title>Photogur</title>
	<%= stylesheet_link_tag    'application', media: 'all' %>
	<%= javascript_include_tag 'application' %>
	<%= csrf_meta_tags %>
</head>
<body>
	<%=flash.notice%>
	<%=flash.alert%>
	<p class="userinfo">
		<% if current_user %>
			 Signed in as <%= current_user.email %> . <%= link_to "Log out","/sessions", :method => :delete %>
		<% else %>
			<%=link_to "Log in", "/sessions/new" %> or <%= link_to"Sign Up",  "/users/new" %>
		<% end %>
	</p>
	<%= yield %>


</body>
</html>
